struct Counter {
    count: i32,
}
impl Counter {
    fn new() -> Self {
        Self { count: 0 }
    }

    fn increment(&mut self) {
        self.count += 1;
    }

    fn get_count(&self) -> i32 {
        self.count
    }
}
fn main() {
    let mut counter : Counter = Counter::new();
    counter.increment();
    counter.increment();
    printInt(counter.get_count());
    exit(0)
}