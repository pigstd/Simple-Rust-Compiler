fn main() {
    let temperature: i32 = 25;
    if (temperature > 30) {
        let status_: &str = "hot";
    } else if (temperature < 10) {
        let status_: &str = "cold";
    } else {
        let status_: &str = "mild";
    }
    exit(0);
}
