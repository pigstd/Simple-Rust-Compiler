; EXPECT: coverage for bitwise, shift, and division instructions
target triple = "unknown-unknown-unknown"
target datalayout = ""

define i32 @bitwise_ops(i32 %a, i32 %b) {
entry.0:
    %and.0 = and i32 %a, %b
    %or.0 = or i32 %a, %b
    %xor.0 = xor i32 %a, %b
    %shl.0 = shl i32 %a, 2
    %lshr.0 = lshr i32 %a, 1
    %ashr.0 = ashr i32 %a, 1
    %sdiv.0 = sdiv i32 %a, %b
    %udiv.0 = udiv i32 %a, %b
    %srem.0 = srem i32 %a, %b
    %urem.0 = urem i32 %a, %b
    %tmp0.0 = add i32 %and.0, %or.0
    %tmp1.0 = add i32 %tmp0.0, %xor.0
    %tmp2.0 = add i32 %tmp1.0, %shl.0
    %tmp3.0 = add i32 %tmp2.0, %lshr.0
    %tmp4.0 = add i32 %tmp3.0, %ashr.0
    %tmp5.0 = add i32 %tmp4.0, %sdiv.0
    %tmp6.0 = add i32 %tmp5.0, %udiv.0
    %tmp7.0 = add i32 %tmp6.0, %srem.0
    %sum.0 = add i32 %tmp7.0, %urem.0
    ret i32 %sum.0
}
