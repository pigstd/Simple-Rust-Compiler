// 测试复杂的表达式，和数组

fn main() {
    let x : i32 = 1 + 2 * (3 - 4) / 5;
    let arr1 : [i32; 3] = [1, 2, 3];
    let arr2 : [i32; 5] = [0; 5];
    let y : i32 = arr1[0] + arr2[1];
    let arr3 : [[i32; 2]; 2] = [[1, 2], [3, 4]];
    let z : i32 = arr3[0][1];
    let tmp : u32 = (x + y) * z as u32;
    println("{}", tmp);
    exit(0);
}