; EXPECT: print builtin called with &str literal
target triple = "unknown-unknown-unknown"
target datalayout = ""

@.str.0 = private constant [3 x i8] c"hi\00"

declare void @print({ ptr, i32 })

define i32 @main() {
entry.0:
    %tmp.0 = alloca { ptr, i32 }
    %ptr.field = getelementptr { ptr, i32 }, ptr %tmp.0, i32 0, i32 0
    %len.field = getelementptr { ptr, i32 }, ptr %tmp.0, i32 0, i32 1
    %msg.ptr = getelementptr [3 x i8], ptr @.str.0, i32 0, i32 0
    store ptr %msg.ptr, ptr %ptr.field
    store i32 2, ptr %len.field
    %tmp.1 = load { ptr, i32 }, ptr %tmp.0
    call void @print({ ptr, i32 } %tmp.1)
    ret i32 0
}
