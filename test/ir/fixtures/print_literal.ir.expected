; EXPECT: print builtin called with &str literal
target triple = "unknown-unknown-unknown"
target datalayout = ""

%StrLiteral = type { ptr, i32 }

@.str.0 = private constant [3 x i8] c"hi\00"

declare void @print(%StrLiteral)

define i32 @main() {
entry.0:
    %tmp.0 = alloca %StrLiteral
    %ptr.field = getelementptr %StrLiteral, ptr %tmp.0, i32 0, i32 0
    %len.field = getelementptr %StrLiteral, ptr %tmp.0, i32 0, i32 1
    %msg.ptr = getelementptr [3 x i8], ptr @.str.0, i32 0, i32 0
    store ptr %msg.ptr, ptr %ptr.field
    store i32 2, ptr %len.field
    %tmp.1 = load %StrLiteral, ptr %tmp.0
    call void @print(%StrLiteral %tmp.1)
    ret i32 0
}
